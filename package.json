{
  "name": "holonet",
  "version": "0.0.0",
  "description": "Mail Delivery System",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "build": "webpack --config ./webpack/prod.config.js --progress --colors -p",
    "watch": "webpack-dev-server --config ./webpack/local.config.js --hot --port 3000",
    "test": "npm run lint",
    "lint": "eslint holonet/frontend",
    "start": "npm run watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webkom/holonet.git"
  },
  "keywords": [
    "Mail"
  ],
  "author": "Eirik Martiniussen Sylliaas <eirik@sylliaas.no>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/webkom/holonet/issues"
  },
  "homepage": "https://github.com/webkom/holonet#readme",
  "devDependencies": {
    "babel-core": "6.4.5",
    "babel-eslint": "4.1.6",
    "babel-loader": "6.2.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-polyfill": "6.3.14",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babel-preset-stage-0": "6.3.13",
    "eslint": "1.10.3",
    "css-loader": "0.23.1",
    "eslint-config-airbnb": "4.0.0",
    "eslint-config-webkom": "1.2.0",
    "eslint-plugin-react": "3.16.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "nib": "1.1.0",
    "react-hot-loader": "1.3.0",
    "style-loader": "0.13.0",
    "stylus": "0.53.0",
    "stylus-loader": "1.5.1",
    "webpack": "1.12.12",
    "webpack-bundle-tracker": "0.0.93",
    "webpack-dev-server": "1.14.1",
    "json-loader": "0.5.4"
  },
  "dependencies": {
    "camelize": "1.0.0",
    "history": "1.17.0",
    "moment": "2.10.6",
    "moment-timezone": "0.5.0",
    "normalize.css": "3.0.3",
    "react": "0.14.7",
    "react-dom": "0.14.7",
    "react-fa": "4.0.0",
    "react-redux": "4.1.2",
    "react-router": "1.0.3",
    "redux": "3.1.7",
    "redux-logger": "2.4.0",
    "redux-promise": "0.6.0-alpha",
    "redux-router": "1.0.0-beta7",
    "redux-thunk": "1.0.3",
    "superagent": "1.7.2"
  },
  "eslintConfig": {
    "extends": "webkom/react",
    "parser": "babel-eslint",
    "rules": {
      "new-cap": 0
    }
  },
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-decorators-legacy"
    ]
  }
}
